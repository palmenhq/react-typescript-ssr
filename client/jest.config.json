{
  "transform": {
    "\\.tsx?$": "<rootDir>/test-preprocessor.js"
  },
  "testRegex": "\\.test\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "setupFiles": [
    "<rootDir>/test-setup-enzyme.js"
  ],
  "moduleNameMapper": {
    "\\.(svg|png|gif|jpeg)$": "<rootDir>/test-file-mock.js",
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "globals": {
    "ts-jest": {
      "tsConfigFile": "./tsconfig.json"
    }
  }
}
